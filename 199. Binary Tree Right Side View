class Solution:
    def rightSideView(self, root: TreeNode) -> List[int]:
        res = []
        if root == None:
            return res
        q = []
        
        q.append(root)
        while q:
            size = len(q)
            for i in range(size):
                curr = q.pop(0)
                if i == (size -1):
                    res.append(curr.val)
                if curr.left:
                    q.append(curr.left)
                if curr.right:
                    q.append(curr.right)
        return res
        
        
