class RandomizedSet:

    def __init__(self):
        self.arr = []
        

    def insert(self, val: int) -> bool:
        if val not in self.arr:
            self.arr.append(val)
            return True
        else:
            return False
        
        

    def remove(self, val: int) -> bool:
        if val in self.arr:
            self.arr.remove(val)
            return True
        else:
            return False

    def getRandom(self) -> int:
        k = random.choice(self.arr)
        
        return k
        
