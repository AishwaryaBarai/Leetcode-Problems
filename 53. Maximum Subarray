class Solution:
    def maxSubArray(self, nums: List[int]) -> int:
        if nums == None:
            return 0
        else:
            max_sum = total_sum = nums[0]
            for i in nums[1:]:
                total_sum = max(total_sum + i, i)
                max_sum = max(max_sum, total_sum)
            return max_sum
