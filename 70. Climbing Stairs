using hashmap:

class Solution:
    def climbStairs(self, n: int) -> int:
        if n <=2:
            return n
        else:
            dict = {1:1,2:2}
            for i in range(3,n+1):
                dict[i] = dict[i-1] + dict[i-2]
        return dict[n]
            
        

class Solution:
    def climbStairs(self, n: int) -> int:
        if n <=1:
            return n
        if n >= 2:
            d = {1:1, 2:2}
            for i in range(3,n+1):
                curr = d[i-1] + d[i-2]
                d[i] = curr
            return d[n]
            
            
            
 class Solution:
    def climbStairs(self, n: int) -> int:
        if n <=1:
            return n
        if n >= 2:
            curr = 2
            prev = 1
            for i in range(3,n+1):
                curr = curr + prev
                prev = curr - prev
            return curr
            
            
        
            
