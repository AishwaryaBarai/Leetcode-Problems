naive:
class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        for i in range(len(nums)):
            val = target - nums[i]
            if (val in nums) and i != nums.index(val):
                return i,nums.index(val)
        

dict:

class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        dic = {}
        len_nums = len(nums)
        i = 0
        while i < len(nums):
            val = target - nums[i]
            if val in dic:
                return(i, dic[val])
            dic[nums[i]] = i
            i +=1
        
        
