class Solution:
    def isValid(self, s):
        stck = []
        dict = {"]":"[", "}":"{", ")":"("}
        for char in s:
            if char in dict.values():
                stck.append(char)
            elif char in dict.keys():
                if stck == [] or dict[char] != stck.pop():
                    return False
            else:
                return False
        return stck == []
