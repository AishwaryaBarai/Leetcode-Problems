class Solution:
    def removeNthFromEnd(self, head: ListNode, n: int) -> ListNode:
        dummy = ListNode(0)
        dummy.next = head
        first = dummy
        second = dummy
        for i in range(1,n+1):
            first = first.next
        while first.next:
            first = first.next
            second = second.next
        second.next = second.next.next
        return dummy.next
        


dummy = ListNode(0, head)
	f, s = dummy, dummy
	while n:
		f = f.next
		n-=1

	while f and f.next:
		f = f.next
		s = s.next
	s.next = s.next.next
	return dummy.next
