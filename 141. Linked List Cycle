class Solution:
    def hasCycle(self, head: ListNode) -> bool:
        if head == None:
            return 0
        else:
            slow = head
            fast = head
            while(fast !=None and fast.next !=None):
                slow = slow.next
                fast = fast.next.next
                if(slow == fast):
                    return 1
        return 0
        
