class Solution:
    def countSubstrings(self, s: str) -> int:
        res = 0
        if not s:
            return res
        for i in range(len(s)):
            res+=1
            for j in range(1,i+1):
                if self.palindrome(s[i-j:i+1]):
                    res+=1
        return res
    def palindrome(self,a):
        return a==a[::-1]
                
                
