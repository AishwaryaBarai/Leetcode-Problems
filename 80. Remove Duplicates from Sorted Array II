class Solution:
    def removeDuplicates(self, nums: List[int]) -> int:
        count=0
        idx=0
        for i in range(len(nums)):
            if count<2:
                nums[idx] = nums[i]
                idx+=1
            if i == len(nums)-1 or nums[i]!=nums[i+1]:
                count=0
            elif nums[i]==nums[i+1]:
                count+=1
        return idx
        
