DP:

class Solution:
    def rob(self, nums: List[int]) -> int:
        prev1,prev2 = 0,0
        for i in nums:
            cur = max(prev1,i+prev2)
            prev2 = prev1
            prev1 = cur

        return prev1


class Solution:
    def rob(self, nums: List[int]) -> int:     
        for i in range(2, len(nums)):
            nums[i] += max(nums[:i-1])
        return max(nums) if nums else 0
        
