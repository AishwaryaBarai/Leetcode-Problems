class Solution:
    def pathSum(self, root: TreeNode, targetSum: int) -> List[List[int]]:
        if not root:
            return []
        res = []
        stack = [(root,[root.val])]
        
        while stack:
            curr,ls = stack.pop()
            if not curr.left and not curr.right and sum(ls) == targetSum:
                res.append(ls)
                
            if curr.left:
                stack.append((curr.left, ls+[curr.left.val]))
            if curr.right:
                stack.append((curr.right, ls + [curr.right.val]))    
                
        return res
        
        
