Time: o(1)
space: o(n)


class MinStack:

    def __init__(self):
        self.arr = []

    def push(self, x: int) -> None:
        if self.arr:
            self.arr.append((x,min(x,self.getMin())))
        else:
            self.arr.append((x,x))
        

    def pop(self) -> None:
        if self.arr:
            self.arr.pop()
        

    def top(self) -> int:
        return self.arr[-1][0]
        

    def getMin(self) -> int:
        return self.arr[-1][1]
        
