class Solution:
    def reverseWords(self, s: str) -> str:
        #print(s)
        s = s.split()
        #print(s)
        i = 0
        j = len(s) - 1
        while i < j:
            s[i], s[j] = s[j], s[i]
            i+=1
            j-=1
        final_str = ''
        for i in s:
            final_str += ' ' + i
        return (final_str.strip())
            
       # print(final_str)
        
