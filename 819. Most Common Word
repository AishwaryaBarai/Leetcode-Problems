class Solution:
    def mostCommonWord(self, paragraph: str, banned: List[str]) -> str:
        for c in "!?',;.": 
            paragraph = paragraph.replace(c, " ")
            
        cpara = collections.Counter(paragraph.lower().split())
        banned = set(banned)
        
        for word,_ in cpara.most_common():
            if word not in banned:
                return word
        
