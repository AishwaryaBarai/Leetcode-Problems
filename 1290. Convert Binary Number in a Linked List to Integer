class Solution:
    def getDecimalValue(self, head: ListNode) -> int:
        temp = ''
        while head !=None:
            temp += str(head.val)
            head = head.next
        return int(temp,2)
        
        
        
 class Solution:
    def getDecimalValue(self, head: ListNode) -> int:
        lst = []
        while head:
            lst.append(head.val)
            head = head.next
        res = int("".join(str(x) for x in lst), 2)
        return res
            
